{% extends "base.twig" %}
{% block content %}
<h1> Signup </h1>
<form method="post">
  {% for x in message %}
    {{ x }}
  {% endfor %}
  <label for="username">Username: </label>
  <input type="text" id="username" name="username" value="{{ post.username|e }}" /><br />
  <label for="password">Password: </label>
  <input type="password" id="password" name="password" /><br />
  <button>
    Register
  </button>
  {{ insert_token() }}
</form>
{% endblock %}
